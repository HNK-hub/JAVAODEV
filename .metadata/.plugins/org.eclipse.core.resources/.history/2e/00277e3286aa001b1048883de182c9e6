
public class Main {

	public static void main(String[] args) {
		Day Day1 = new Day(); // referans oluþturma
		Day1.id=1;
		Day1.Dayname = "1.Gün";
		Day1.Date ="21 Nisan 2021" ;
		Day1.detail="16 GB Ram ";
		
		Day Day2 = new Day(); 
		Day2.id=2;
		Day2.Dayname = "2.Gün";
		Day1.Date ="24 Nisan 2021" ;
		Day2.detail=" ";
		
	
		
		Day [] Days = {Day1};
		   for (Day Day : Days) 
		   {
			   System.out.println(Day.Dayname);
		   }
		   System.out.println(Days.length);
		   
		
	}

}
